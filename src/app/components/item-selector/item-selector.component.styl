.item-selector {
  background-color: #fff
  border-radius: 3px;
  .schedule-item {
    display: flex
    justify-content: space-between;
    padding: 10px 0;
    border-bottom: 1px solid #999;

    .wrapper-left {
      display: flex
      align-items: center;
    }
  }

  .empty-wrapper {
    line-height: 32px
  }
}
